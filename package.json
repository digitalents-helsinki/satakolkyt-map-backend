{
  "name": "satakolkyt-map-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "db:start": "./start.sh satakolkyt",
    "build": "tsc",
    "watch": "yarn build --watch",
    "nodemon": "nodemon --signal SIGHUP dist/index.js -q",
    "dev": "yarn run build && run-p nodemon watch"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "src/*"
    ],
    "delay": 1000,
    "verbose": false
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.5",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "arangojs": "^6.10.0",
    "body-parser": "^1.18.3",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "module-alias": "^2.2.0"
  }
}
